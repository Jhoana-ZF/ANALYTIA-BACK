// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource sqlite {
  provider = "sqlite"
  url      = 'file:../analytia.db'
}

model User {
  id    Int      @id @default(autoincrement())
  nombre String
  email  String   @unique
  edad   Int
}
  
